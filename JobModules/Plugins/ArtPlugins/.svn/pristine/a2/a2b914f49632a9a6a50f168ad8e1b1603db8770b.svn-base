using System.Collections;
using System.Collections.Generic;
using UnityEngine;

namespace ArtPlugins
{
    public class SightInnerMask : MonoBehaviour
    {
        public Renderer sightRender;


        private void OnEnable()
        {
            if (sightRender != null)
                sightRender.material.renderQueue = 3001;
        }
        private void OnDisable()
        {
            if (sightRender != null)
                sightRender.material.renderQueue = sightRender.material.shader.renderQueue;
        }
    }

}